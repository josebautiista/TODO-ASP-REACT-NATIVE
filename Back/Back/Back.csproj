    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>b4693dc7-1152-4d3c-9a15-cb090f3d7d21</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="8.0.8">
